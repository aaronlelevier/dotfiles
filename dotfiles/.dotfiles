# prompt
function parse_git_branch() {
    git branch 2> /dev/null | sed -n -e 's/^\* \(.*\)/(\1)/p'
}

COLOR_DEF=$'\e[0m'
COLOR_USR=$'\e[0;0;0m'
COLOR_DIR=$'\e[38;5;25m'
COLOR_GIT=$'\e[38;5;200m'
export PROMPT='${COLOR_USR}%n ${COLOR_DIR}%1~ ${COLOR_GIT}$(parse_git_branch)${COLOR_DEF} $ '

# links
source ~/.aliases
source ~/.functions
source ~/.gitshortcuts

# golang
export GO111MODULE="on" 
